/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
image {
  display: block;
  width: 100%;
}

::-webkit-scrollbar{width:10px;height:10px}
::-webkit-scrollbar-thumb{background:#c1c1c1;-webkit-border-radius:10px;border-radius:10px}
::-webkit-scrollbar-track{background-color:#f1f1f1}
input::-webkit-input-placeholder,textarea::-webkit-input-placeholder{color:#ccc;font-weight:300}
input:-moz-placeholder,textarea:-moz-placeholder{color:#ccc;font-weight:300}
input::-moz-placeholder,textarea::-moz-placeholder{color:#ccc;font-weight:300}
input:-ms-input-placeholder,textarea:-ms-input-placeholder{color:#ccc;font-weight:300}
input.placeholder,textarea.placeholder{color:#ccc;font-weight:300}
.fixed{position:fixed!important}
.opacity{opacity:.5;filter:alpha(opacity=50)}
.underline{display:inline-block;line-height:1.2;border-bottom:1px dotted}
.ellipsis{white-space:nowrap;overflow:hidden;-o-text-overflow:ellipsis;text-overflow:ellipsis}
.pointer{cursor:pointer}
.theh {height: 100rpx}



.main-container {
  padding:0;
  box-sizing: border-box;
  width: 100%;
}

.success-rate {
  
  height: 34rpx;
  display: flex;
  margin-bottom: 10rpx;
  }

.success-rate>.left{
  background: linear-gradient(-161deg, #CB72FF 0%, #A359FF 100%);
  width: 86rpx;
  height: 38rpx;
  text-align: center;
  font-size: 22rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  line-height: 38rpx;
  border-top-left-radius: 4rpx;
  border-bottom-left-radius: 4rpx;
  
}

.success-rate>.right{
 width: 65rpx;
  height: 34rpx;
 border: 1rpx solid #9179FF;
 border-left: none;
 font-size: 22rpx;
 color: #9179FF;
 line-height: 34rpx;
 text-align: center;
 border-top-right-radius: 4rpx;
 border-bottom-right-radius: 4rpx;
 
}
.stars {
  display: flex;
}

.star {
  width: 24rpx;
  height: 24rpx;
  display: block;
  background: yellow;
  margin-right: 10rpx;
}
